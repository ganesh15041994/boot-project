<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<!-- In order to turn on Annotations in Spring over xml configuration we 
		need to provide a tag by name <context:annotation-config /> see the tag below 
		.... -->

	<!-- For Enable All Anotaion We use This Tag -->
	<context:annotation-config />


	<bean name="a" class="com.org.spr.Address">
		<property name="city" value="Pune" />
	</bean>
	
	 <bean id="temp" class="com.org.spr.Address">
	 <property name = "city" value="Mumbai"/>
	 </bean>
    
	<bean name="emp" class="com.org.spr.Employee">
		<property name="name" value="Ganesh" />
	</bean>
</beans>